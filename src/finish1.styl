.page-revoke{
    .icon-finish{
        height: 76px;
        width: 76px;
        display: inline-block;
        margin-bottom: 6px;
        background: url(./img/icon_0013.png) no-repeat center;
        margin-top: 50px;
    }
    .notice-text{
        margin-top: 20px;
        line-height: 2;
        color: #A4A8AB;
    }
    .finish-text{
        font-size: 20px;
        color: #50C1FF;
    }
    .revoke-info{
        margin-top: 28px;
    }
    .revoke-info-table{
        margin-top: 10px;
        table{
            width: 358px;
            text-align: left;
            margin: 0 auto;
            border: 1px solid #E6E8EE;
            border-radius: 6px;
            border-collapse: separate;
            td{
                border-right: 1px solid #E6E8EE;
                border-bottom: 1px solid #E6E8EE;
                line-height: 42px;
                padding-left: 1em;
                font-size: 14px;
                color: #A4A8AB;
                &:last-child{
                    border-right: none;
                }
            }
            tr:last-child td{
                border-bottom: none;
            }
        }
    }
    .revoke-title{
        color: #1D1D1D;
        line-height: 32px;
    }
    .btn-my-financing{
        width: 240px;
        margin: 32px auto 60px;
    }
}
